{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/api/demo-requests",
      "dest": "/api/demo-requests.ts"
    },
    {
      "src": "/api/roi-calculation", 
      "dest": "/api/roi-calculation.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}